import pandas as pd

# Load supply chain data
supply_chain_data = pd.read_csv('supply_chain_data.csv')

# Data preprocessing
supply_chain_data['Order Fulfillment Accuracy'] = supply_chain_data['Orders Fulfilled'] / supply_chain_data['Orders Placed']

